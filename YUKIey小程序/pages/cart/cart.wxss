
.cart-list{
    position: relative;
    padding: 20rpx 20rpx 20rpx 260rpx; 
    height: 185rpx;
    border-bottom: 1rpx solid #e9e9e9;
}
.understock{
  position: absolute;
  width:150rpx;
  height:130rpx;
  bottom:0;
  right:60rpx;
}
.cart-list .cart-pro-select{
    position: absolute;
    left: 20rpx;
    top: 90rpx;
    width: 45rpx;
    height: 45rpx;
}

.cart-list .cart-thumb{
    position: absolute;
    top: 20rpx;
    left: 70rpx;
    width: 185rpx;
    height: 185rpx;
}
.cart-list .cart-pro-name{
    display: inline-block;
    width:480rpx;
    line-height: 50rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow:ellipsis;
    font-size: 16px;
}
.cart-list .cart-pro-style{
    display: inline-block;
    width:480rpx;
    font-size: 14px;
    color:#666;
    line-height: 50rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow:ellipsis;
}
.cart-list .cart-pro-styled{
    display: inline-block;
    margin-top: 50rpx;
    text-align: right;
    width:480rpx;
    font-size: 14px;
    color:#666;
    line-height: 50rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow:ellipsis;
}
.cart-list .cart-pro-price{
    display: inline-block;
    float: left;
    color:red;
    font-size: 14px;
    line-height: 50rpx;
}
.cart-list .cart-pro-count{
    display: inline-block;
    float: right;
    font-size: 14px;
    line-height: 50rpx;
}
.cart-list .cart-count-box{
  float: right;
  border: 1px solid #666;
  border-radius: 5px;
  right: 20rpx;
  bottom: 10rpx;
  width: 200rpx;
  height: 60rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.cart-list .cart-count-box text{
    display: block;
    line-height: 60rpx;
    text-align: center;
}
.cart-count-down,.cart-count-add{
    font-size: 44rpx;
    width: 50rpx;
    height: 100%;
}
.cart-count-num{
    width: 96rpx;
    font-weight: normal;
    border-left: 1px solid #666;
    border-right: 1px solid #666;
}
.cart-footer{
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 90rpx;
    line-height: 90rpx;
    box-sizing: border-box;
    border-top:1px solid #999;
    color: #000;
    background: #fff;
}
.total-select{
    position: absolute;
    left: 20rpx;
    top: 25rpx;
    width: 45rpx;
    height: 45rpx;
}
.order-icon{
    position: absolute;
    right: 40rpx;
    top: 25rpx;
    width: 45rpx;
    height: 45rpx;
}
.order-icon image,.order-icon navigator{
  display: block;
  width: 45rpx;
  height: 45rpx;
}
.cart-toatl-price{
    float: left;
    margin-left: 20rpx;
    font-size: 16px;
}

.weui-cell__bd{
  margin-left: 25px;
}
.weui-cell__ft{
  color:black;
}
.button-blue {
  background-color: #4595e5; /* 蓝色 */
  font-size: 16px
}
button {
  position: fixed;
  right: 0;
  color: white;
  text-align: center;
  display: inline-block;
  font-size: 30rpx;
  border-radius: 0rpx;
  width: 30%;
  height: 100rpx;
  line-height: 100rpx;
}
.list-box{
    padding-left:15rpx;
    background: #f0f0f0
}
.guesslike{
  height:80rpx;
  text-align: center;
  font-size: 32rpx;
  background: #f0f0f0;
  line-height: 80rpx;
}
.newest-list{
    display: inline-block;
    width: 344rpx;
    height: 456rpx;
    margin-bottom: 20rpx;
    margin-left: 10rpx;
    margin-right: 10rpx;
    text-align: center;
    background: #fff;
}
.newest-list:nth-child(2n){
    margin-right: 0;
}
.newest-list image{
    width: 320rpx;
    height: 300rpx;
    margin: 10rpx 0 20rpx;
}
.newest-list .newest-text{
    font-size: 32rpx;
    line-height: 45rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow:ellipsis;
    width:320rpx;
}
.newest-list .newest-price{
    font-size: 32rpx;
    line-height: 45rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow:ellipsis;
    width:320rpx;
    text-align: left;
    color: red;
}
.diandi{
  height:100rpx;
  background: #f0f0f0;
}
.nogoods{
  height:250px;
  text-align:center;
}
.nogoods image{
  margin: 20rpx;
  width:320rpx;
  height:320rpx;
}
.cart-no-data{
  color: #999;
  text-align: center;
}