  .clearfix:after { content: ""; display: block; clear: both; }
  .clearfix { zoom: 1; }
  page{
    background:#fff;
  }
  .container{
    background:#fff;
  }
  .mask{
    position: fixed;
    top:0;
    left:0;
    background:#000;
    opacity: 0.6;
    width:100%;
    height: 100%;
    z-index: 99;
  }
  .mask image,.mask span{
    position: fixed;
    left:50%;
    top:50%;
    margin-left:-300rpx;
    margin-top:-300rpx;
    width:600rpx;
    height: 600rpx;
    font-size:35rpx;
    color:#fff;
    text-align: center;
    line-height: 600rpx;
  }
  .containerContent{
    background:#fff;
  }
  .goodIntro{
  	height: 128rpx;
  	padding: 24rpx 32rpx;
  }
  .goodIntro .goodIntro_img,.goodIntro .goodIntro_des{
    text-align: left;
  	float: left;
  }
  .goodIntro_img{
  	width: 120rpx;
  	height: 120rpx;
  	margin-right: 24rpx;
  }
  .goodIntro_img image{
    width:100%;
    height: 100%;
  }
  .goodIntro_des{
  	position: relative;
  	width: 542rpx;
  	height: 120rpx;
  }
  .goodIntro_des .goodIntro_des_F{
  	font-size: 26rpx;
  	color: #333;
  	line-height: 32rpx;
  }
  .goodIntro_des .goodIntro_des_S{
  	position: absolute;
  	bottom: 0;
  	font-size: 26rpx;
  	color: #e54560;
  }
  .choooseIcon{
  	height: 128rpx;
  	width: 100%;
  	display: -webkit-flex;
  	display: -moz-flex;
  	display: -ms-flex;
  	display: flex;
  }
  .choooseIcons {
  	flex: 1;
  	display: flex;
  	justify-content:center;
  	align-items:center;
  }
.choooseIcons span{
  	display: inline-block;
  }
 .choooseIcons span.choooseIconsF {
  	width: 50rpx;
  	height: 50rpx;
  	margin-right: 24rpx;
  }
 .choooseIcons span.choooseIconsS{
  	font-size: 28rpx;
  	color: #666;
  }
  .choooseIcons.choooseIconI.active span.choooseIconsS{
    color: #e54560;
  }
.choooseIcons.active.choooseIconII span.choooseIconsS{
    color: #ff9600;
  }
.choooseIcons.active.choooseIconIII span.choooseIconsS{
    color: #000;
  }
  .choooseIconI .choooseIconsF{
  	background: url(data:image/png;base64,);
