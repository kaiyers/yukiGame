  .write{
        position:relative;
  }
  .writeIn{
  	width: 590rpx;
  	margin: 0 auto 24rpx;
  	padding: 48rpx;
  	background: #fafafa;
  	border-radius: 12rpx;
  	overflow-y:auto;
  	font-size: 32rpx;
  }
  .write span{
    position: absolute;
    width:45rpx;
    height: 45rpx;
    bottom:0;
    right:32rpx;
    font-size: 28rpx;
    color:#333;
    text-align: center;
    line-height: 45rpx;
  }
  .filetitle{
  	display: block;
  	font-size: 30rpx;
    padding-left: 32rpx;
    padding-right: 32rpx;
  	text-align: left;
    color: #333;
  	height: 92rpx;
  	line-height: 92rpx;
  }
  button.issue{
    width:50%;
  	height: 88rpx;
  	color: #f47920;
  	font-size: 44rpx;
  	line-height: 88rpx;
    margin-bottom:30rpx;
    margin-top:30rpx;
    text-align: center;
    outline:none;
    border: 1px solid #e5e5e5; 
  }
  button.issue::after{
    border:none;
  }
  .fileIfPIC{
    padding-left: 9rpx;
    padding-right: 9rpx;
    padding-bottom:30rpx;
  }
  .spacing{
    height: 30rpx;
    background:#eee;
  }
  .fileIfPICView{
    position: relative;
    height: 100rpx;
    width: 100rpx;
    float: left;
    margin-left: 23rpx;
    margin-right:23rpx;
    background: #fafafa;
    border: 1px dashed #d9d9d9;
  }
  .fileIfPICView span{
    position: absolute;
    width: 40rpx;
    height: 40rpx;
    top: -20rpx;
    right: -20rpx;
    background: url(data:image/png;base64,);
    background-repeat:round;
  }
  #choose{
    display: none;
  }
  .pushL{
    position: relative;
    height: 100rpx;
    width: 100rpx;
    float: left;
    font-size:80rpx;
    font-weight: 900;
    color:#000;
    margin-left: 20rpx;
    margin-right: 20rpx;
    background: #fafafa;
    border: 1px dashed #d9d9d9;
    text-align: center;
    line-height: 94rpx;
  }
  textarea{
    color:#000;
  }
  .pushL image{
    width:100%;
    height: 100%;
  }
  .pushL span{
    position: absolute;
    width: 40rpx;
    height: 40rpx;
