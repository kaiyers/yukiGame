.headerSeach{
  position:fixed;
  top:0;
  height: 90rpx;
  width:690rpx;
  padding-left:30rpx;
  padding-right: 30rpx;
  z-index: 99;
}
.headerSeachLeft{
  position: relative;
  height: 66rpx;
  float:left;
  padding-left: 30rpx;
  overflow: hidden;
  width: 386rpx;
  border: 2rpx solid #d4d4d4;
  border-radius: 64rpx;
  margin: 10rpx 0;
  background: #EBEBEB;
}
.headerSeachLeft icon{
  position: absolute;
  top: 16rpx;
  left: 16rpx;
  width: 32rpx;
  height: 32rpx;
}
.headerSeachLeftText{
  color: #8f8f94;
    height: 66rpx;
    line-height: 66rpx;
    text-align: left;
    font-size: 30rpx;
    padding-left: 30rpx;
}
.headerSeachRight{
  float: right;
  height: 90rpx;
  font-size: 40rpx;
  color:#007aff;
  line-height: 90rpx;
}
.SeachContents{
  margin-top:90rpx;
}
.historySeach,.hotSeach{
  padding:10rpx 30rpx;
}
.historySeachTitle,.hotSeachTitle{
  height: 68rpx;
  margin-bottom: 40rpx;
}
.historySeachTitleL{
  float:left;
  height:68rpx;
  font-size:40rpx;
  line-height:68rpx;
  color:#000;
}
.historySeachTitleR{
  float:right;
  margin-top:10rpx;
  width:48rpx;
  height:48rpx;
}
.historySeach navigator,.hotSeach navigator{
  float:left;
  font-size: 26rpx;
  line-height: 48rpx;
  color: #666;
  text-align: left;
  padding: 0 16rpx;
  border: 2rpx solid #808080;
  border-radius: 2rpx;
  margin: 0 30rpx 30rpx 0;
}