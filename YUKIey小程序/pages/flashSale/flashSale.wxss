.container{
  overflow: hidden;
}
.guessLikeContain{
  width: 100%;
  overflow: hidden;
}
swiper {
  height:420rpx;
  width:100%;
  overflow:hidden;
}
swiper-item{
  overflow-y: auto;
}
.swiper-tab{  
  position:fixed;
  top:0;
  left:0; 
  height: 80rpx;
  display:-webkit-box;
  z-index: 99;
  width: 100%;   
  text-align: center;  
  line-height: 80rpx;
  background:#fff;
  }  
.swiper-tab-list{ 
  -webkit-box-flex:1;
  font-size: 30rpx;   
  color: #777777;  
}  
.on{ 
  color: #26a2ff;
  margin-bottom: 3px;
  border-bottom: 3px solid #26a2ff;
}  
.swiper-box{ 
  display: block; 
  height: 100%;
  /* padding-left:30rpx;
  padding-right:30rpx; */
  overflow: hidden;
}  
.swiper-box view{  
    text-align: center;  
}
swiper image {
  width:100%;
  height:420rpx;
}
.limitTimeMes{
  padding: 15rpx; 
  background: rgb(255, 255, 255); 
  font-size: 30rpx; 
  font-weight: bolder;
  margin-bottom:20rpx;
  text-align:center;
}
.limitGoods{
  padding: 20rpx;
  background: #fff;
  border-bottom: 1px solid #b6b6b6;
}
.limitGoods.limitGoodN{
  border:none;
}
.limitGoodsImage{
    width: 280rpx;
    height: 280rpx;
    margin-right:20rpx;
    position: relative;
}
.limitGoodsImage view{
    position: absolute;
    left:0;
    top: 0;
    width: 280rpx;
    height:280rpx;
    line-height: 280rpx;
    border-radius: 50%;
    background: rgba(0,0,0,.3);
    font-weight: bolder;
    font-size: 40rpx;
    color: #fff;
}
.limitGoodsMes{
    width: 400rpx;
    text-align: left;
}
.saleListGoodsName{
  margin-bottom: 15rpx;
  font-weight: bolder;
  font-size: 30rpx;
  text-align: left;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.limPrice view{
  color: #e54560;
  font-weight: bolder;
  font-size: 30rpx;
}
.purchasePrice{
  color: #9d9d9d;
  font-size: 20rpx;
  margin: 10rpx 0;
}
.purchasePriceL {
  color: #9d9d9d;
  font-size: 20rpx;
  margin: 10rpx 0;
}
.purchasePriceTh{
  text-decoration: line-through;
}
.limitGoodsMesL{
  text-align: center;
  line-height: 55rpx;
  width: 165rpx;
  height: 55rpx;
  border: 4rpx solid #4595e5;
  display: block;
  color: #4595e5;
  background: #fff;
  border-radius: 50rpx;
  overflow: hidden;
  position: relative;
}
.limitGoodsMesL .progressM{
  position: absolute;
  top:0;
  left:0;
  height: 55rpx;
  background:#dcebfa;
}
.limitGoodsMesL .progressPer{
  position: absolute;
  left: 0;
  top: 0;
  font-size:24rpx;
  width:165rpx;
  line-height: 55rpx;
  color: #4595e5;
  text-align: center;
  z-index: 99;
}
.limitGoodsMesR{
  font-size:24rpx;
  text-align: center;
  line-height: 65rpx;
  width: 165rpx;
  height: 65rpx;
  font-weight: bolder;
  display: block;
  color: #fff;
  background: #7aaae9;
  border-radius: 8rpx;
}
.limitGoodsMesPr{
    width: 400rpx;
    text-align: left;
}
.activeBlank{
  font-size:30rpx;
  color:#000;
  padding:20rpx;
}
  