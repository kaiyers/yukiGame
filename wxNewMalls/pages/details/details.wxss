/*普通样式*/
.stepper .minNormal{ 
  color: black;
  border: 2px solid #666666;
  border-right: 0;
  border-top-left-radius: 44rpx; 
  border-bottom-left-radius: 44rpx; 
} 
  
/*禁用样式*/
.stepper .minDisabled{ 
  color: #cccccc;
  border: 2px solid #cccccc;
  border-right: 0;
  border-top-left-radius: 44rpx; 
  border-bottom-left-radius: 44rpx; 
} 
/*普通样式*/
.stepper .maxNormal{ 
  color: black;
  border: 2px solid #666666;
  border-left: 0;
  border-top-right-radius: 44rpx; 
  border-bottom-right-radius: 44rpx; 
} 
  
/*禁用样式*/
.stepper .maxDisabled{ 
  color: #cccccc;
  border: 2px solid #cccccc;
  border-left: 0;
  border-top-right-radius: 44rpx; 
  border-bottom-right-radius: 44rpx; 
} 
