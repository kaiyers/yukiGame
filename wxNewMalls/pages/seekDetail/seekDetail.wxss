page{
  background: #fafafa;
  overflow-x: hidden;
}
.container{
  overflow: hidden;
}
.headerSeach{
  position:fixed;
  top:0;
  height: 90rpx;
  width:690rpx;
  padding-left:30rpx;
  padding-right: 30rpx;
  background:#fff;
  border-bottom:4rpx solid #e0e0e0;
  z-index: 99;
}
.headerSeachLeft{
  position: relative;
  height: 64rpx;
  padding-left: 82rpx;
  overflow: hidden;
  width: 526rpx;
  border-radius: 64rpx;
  margin-top:13rpx;
  background: #e0e0e0;
  margin:13rpx auto 0;
}
.searchIcon{
  position: absolute;
  top: 18rpx;
  left: 32rpx;
  width: 32rpx;
  height: 32rpx;
}
.headerSeachLeftText{
  color: #999;
  height: 64rpx;
  line-height: 64rpx;
  text-align: left;
  font-size: 26rpx;
}
.headerSeachRight{
  float: left;
  width:38rpx;
  height: 38rpx;
  margin-top:26rpx;
}
.guessLikeContain{
  background: none;
  width: 690rpx;
  overflow: hidden;
}
.guessLikeContain image{
  width:336rpx;
  height: 336rpx;
}
.guessLikeContainText1{
  max-width: 312rpx;
  height: 60rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin: 0 16rpx;
  font-size: 28rpx;
  color: #333333;
  line-height: 60rpx;
}
.guessLikeContainText2{
    max-width: 312rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    font-size: 28rpx;
    color: #e54560;
    margin: 0 16rpx;
}
.orderList-null{
    width: 600rpx;
    margin: 0 auto;
    padding-top: 400rpx;
}
.guessLikeContain image.orderList-null-icon{
    display: block;
    width: 300rpx;
    height: 300rpx;
    margin: 0 auto;
}
.orderList-null-title{
    width: 100%;
    margin-top: 20rpx;
    font-size:28rpx;
    line-height: 28rpx;
    color: #666;
    text-align: center;
}
.guessLikeC{
  background:#fafafa;
  padding:30rpx;
}
.newGoodsContainsGood{
  position: relative;
  width: 336rpx;
  height: 480rpx;
  margin-right: 18rpx;
  margin-bottom:32rpx;
  overflow: hidden;
  background:#fff;
  border-radius: 12rpx;
}
.newGoodsContainsText1,.newGoodsContainsText3{
  display: block;
  max-width: 260rpx;
  height: 26rpx;
  margin: 0 18rpx;
  font-size: 26rpx;
  color: #000;
  line-height: 26rpx;
  text-align: left;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.guessLikeC .smllfont{
  margin:-8rpx 18rpx 0;
}
.newGoodsContainsText2{
  display:inline-block;
  height:15rpx;
  font-size: 24rpx;
  color:#000;
  border-bottom:15rpx solid #fff0be;
  line-height:30rpx;
}
.newGoodsContainsText1 {
  margin:12rpx 18rpx 0;
}
.newGoodsContainsText3{
  height: 24rpx;
  font-size:24rpx;
  color:#9b9b9b;
  margin: 10rpx 18rpx 0rpx;
  line-height: 24rpx;
}
image.hearts{
  position:absolute;
  width:40rpx;
  height:40rpx;
  right:34rpx;
  bottom:36rpx;
}
.page__hd{
  height:94rpx;
  margin-top:94rpx;
  background:#fff;
  padding-left:130rpx;
  padding-right: 130rpx;
  border-bottom:4rpx solid #e0e0e0;
  overflow: hidden;
}
.filter {
  margin-top:34rpx;
  height: 28rpx;
}
.filterLeft,.filterMiddle,.filterright{
  width:82rpx;
  height: 28rpx;
  margin-right:114rpx;
  font-size:24rpx;
  color:#666;
  line-height: 28rpx;
}
.filterMiddlePic{
  width:18rpx;
}
.filterrightPic{
  width:24rpx;
}
.filterLeft.active,.filterMiddleW.active,.filterrightW.active{
  color:#ff9700;
}
.filterMiddlePic,.filterrightPic{
  margin-left:8rpx;
  height: 28rpx;
  vertical-align: middle;
}
.filterright{
  margin-right:0;
}
.filterMiddleW,.filterrightW{
  color:#666;
}
.filterMiddle view,.filterright view{
  display: inline-block;
}