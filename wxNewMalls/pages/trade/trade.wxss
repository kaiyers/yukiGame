Page {
  background: #fafafa;
  width: 100%;
  height: 100%;
  overflow-x: hidden;
  overflow-y: auto;
}
.container{
  overflow: hidden;
}
.guessLikeContain{
  position: relative;
  background: none;
  overflow: hidden;
}
.Pics{
  margin-top:340rpx;
  padding-top:32rpx;
  padding-left:30rpx;
  padding-right: 30rpx;
}
.headerPic image{
  width:750rpx;
  height: 322rpx;
}
.headerPic{
  position: absolute;
  top:0;
  left:0;
  width:750rpx;
  height: 322rpx;
  border-top:2rpx solid #eee;
  border-bottom:16rpx solid #eee;
}
.newGoodsContainer {
  width:720rpx;
}
.oldPrice{
  font-size: 24rpx;
  color: #ccc;
  margin-left:16rpx;
  line-height: 36rpx;
  font-style: normal ;
  text-decoration: line-through;
  transform: scale(0.8)
}
.newGoodsContainsGood.noGoodMargin{
  margin-right: 0;
}
