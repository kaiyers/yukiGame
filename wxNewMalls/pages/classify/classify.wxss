page{
  position: fixed;
  height:100%;
  width:100%
}
.headerSeach{
  border-bottom: 1px solid #e0e0e0;
  width:100%;
  height: 90rpx;
  background: #fff;
  z-index: 99;
  overflow: hidden
}
.sheaderSeach{
  padding-left:32rpx;
  padding-right: 32rpx;
  display: flex;
  justify-content: space-between;
}
.headerSeachLeft{
  float: left;
  width:44rpx;
  height: 90rpx;
  margin-right:30rpx;
  overflow: hidden;
}
.headerSeachLeft image{
  display:block;
  margin-top: 29rpx;
  width: 44rpx;
  height: 32rpx;
}
.headerSeachcoupon{
  font-size: 24rpx;
  color:#2c3e50;
  text-align: center;
}
.headerSeachRight{
  position: relative;
  height: 64rpx;
  float:right;
  padding-left: 80rpx;
  overflow: hidden;
  width: 536rpx;
  border-radius: 64rpx;
  margin: 13rpx 0;
  background: #e0e0e0;
}
.searchIcon{
  position: absolute;
  top: 18rpx;
  left: 32rpx;
  width: 32rpx;
  height: 32rpx;
}
.headerSeachRightText{
  height: 64rpx;
  font-size: 26rpx;
  color: #999999;
  text-align: left;
  line-height: 64rpx;
}
.page_bd{
  position: relative
}
/*左侧栏主盒子*/  
.nav_left{
 position: fixed;
 left:0;
 top:98rpx;
 height:100%;
  /*设置行内块级元素（没使用定位）*/   
  width: 225rpx;   
  /*主盒子设置背景色为灰色*/  
  text-align: center; 
  background-color: #fafafa; 
  
}  
/*左侧栏list的item*/
.nav_left .nav_left_items{  
  /*每个高30px*/  
  height: 32px;  
  /*垂直居中*/  
  line-height: 32px;  
  /*再设上下padding增加高度，总高42px*/  
  padding: 8px 0;   
  /*文字14px*/  
  font-size: 24rpx;
  font-weight: bold  
}  
/*左侧栏list的item被选中时*/  
.nav_left .nav_left_items.active{  
  /*字体变成黄色*/ 
  font-weight: bold;
  color:#ffc800; 
  font-size: 28rpx;  
  background: #ffffff 
}  
.biaoji{
  float:left;
  height:64rpx;
  width:10rpx;
  border-radius: 10rpx;
  background: #ffc800;
}
/*右侧栏主盒子*/  
.nav_right{  
  /*右侧盒子使用了浮动*/  
  float: right;
  /*宽度75%，高度占满，并使用百分比布局*/  
  width: 525rpx;   
  padding: 0 28rpx;  
  box-sizing: border-box;  
  height:1010rpx;
}  
.tututu{
  margin-top: 18rpx;
  width:100%;
  height:240rpx
}
.className{
  height:70rpx;
  line-height: 70rpx;
  font-size: 26rpx;
  font-weight: bold
}
/*右侧栏list的item*/  
.nav_right .nav_right_items{  
  /*浮动向左*/  
  float: left;  
  /*每个item设置宽度是33.33%*/  
  width: 33.33%;  
  height: 110px;   
  text-align: center;  
}  
.nav_right .nav_right_items image{  
  /*被图片设置宽高*/  
  width: 50px;  
  height: 60px;  
}  
.nav_right .nav_right_items text{  
  /*给text设成块级元素*/  
  display: block;  
  margin-top: 5px;  
  font-size: 12px;  
  /*设置文字溢出部分为...*/  
  overflow: hidden;  
  white-space: nowrap;  
  text-overflow: ellipsis;  
}  
