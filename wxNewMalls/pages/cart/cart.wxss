/*主容器*/
.stepper {
  margin-left: 190rpx; 
  overflow: hidden;
} 
.stepperpop {
  margin-left: 30rpx; 
  overflow: hidden;
} 
/*加号和减号*/
 .stepper view {
  float: left;
  width: 78rpx; 
  height: 44rpx;
  line-height:44rpx; 
  text-align: center; 
}  
  
/*数值*/
.shuzhi {
  display: block; 
  width: 76rpx; 
  height: 44rpx;
  line-height: 44rpx; 
  text-align: center; 
  font-size: 18rpx; 
  /*给中间的input设置左右边框即可*/
  border: 2px solid #666666; 
} 
  
/*普通样式*/
.stepper .minNormal{ 
  color: black;
  border: 2px solid #666666;
  border-right: 0;
  border-top-left-radius: 44rpx; 
  border-bottom-left-radius: 44rpx; 
} 
  
/*禁用样式*/
.stepper .minDisabled{ 
  color: #cccccc;
  border: 2px solid #cccccc;
  border-right: 0;
  border-top-left-radius: 44rpx; 
  border-bottom-left-radius: 44rpx; 
} 
