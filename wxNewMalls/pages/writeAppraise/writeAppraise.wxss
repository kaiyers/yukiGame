  .container{
    padding-bottom:32rpx;
  }
  .mask{
    position: fixed;
    top:0;
    left:0;
    background:#000;
    opacity: 0.6;
    width:100%;
    height: 100%;
    z-index: 99;
  }
  .mask image,.mask span{
    position: fixed;
    left:50%;
    top:50%;
    margin-left:-300rpx;
    margin-top:-300rpx;
    width:600rpx;
    height: 600rpx;
    font-size:35rpx;
    color:#fff;
    text-align: center;
    line-height: 600rpx;
  }
  .containerContent{
    background:#fff;
  }
  .goodIntro{
  	height: 128rpx;
  	padding: 24rpx 32rpx;
  }
  .goodIntro .goodIntro_img,.goodIntro .goodIntro_des{
    text-align: left;
  	float: left;
  }
  .goodIntro_img{
  	width: 120rpx;
  	height: 120rpx;
  	margin-right: 24rpx;
  }
  .goodIntro_img image{
    width:100%;
    height: 100%;
  }
  .goodIntro_des{
  	position: relative;
  	width: 542rpx;
  	height: 120rpx;
  }
  .goodIntro_des .goodIntro_des_F{
  	font-size: 26rpx;
  	color: #333;
  	line-height: 32rpx;
  }
  .goodIntro_des .goodIntro_des_S{
  	position: absolute;
  	bottom: 0;
  	font-size: 26rpx;
  	color: #e54560;
  }
  .choooseIcon{
  	height: 96rpx;
    border-bottom:2rpx solid #e0e0e0;
  }
  .choooseIcons {
  	flex: 1;
  	display: flex;
  	justify-content:center;
  	align-items:center;
  }
.choooseIcons span{
  	display: inline-block;
    color:#000;
    font-size:26rpx;
    line-height: 96rpx;
  }
 .choooseIcons span.choooseIconsF {
  	width: 32rpx;
  	height: 32rpx;
  	margin-right: 16rpx;
    vertical-align: middle;
  }
  .choooseIcons{
    display: inline-block;
    width:122rpx;
    margin-left:30rpx;
  }
  .choooseIcons .choooseIconsF{
  	background: url(http://yukicomic-pic.oss-cn-hangzhou.aliyuncs.com/xcx/my_collect_icon_empty@3x.png);
  	background-repeat:round;
  }
  .choooseIcons.active .choooseIconsF{
  	background: url(http://yukicomic-pic.oss-cn-hangzhou.aliyuncs.com/xcx/common_icon_chose@3x.png);
  	background-repeat:round;
  }
  .write{
    position:relative;
    padding: 22rpx 30rpx 0;
    height: 242rpx;
  }
  .writeIn{
  	overflow-y:auto;
  	font-size: 26rpx;
    color:#ccc;
    line-height: 30rpx;
  }
  .write span{
    position: absolute;
    width:45rpx;
    height: 45rpx;
    bottom:0;
    right:32rpx;
    font-size: 28rpx;
    color:#333;
    text-align: center;
    line-height: 45rpx;
  }
  button.issue{
    width:686rpx;
  	height: 76rpx;
  	color: #ff9701;
  	font-size: 44rpx;
  	line-height: 76rpx;
    margin-bottom:30rpx;
    margin-top:318rpx;
    text-align: center;
    outline:none;
    border: 2rpx solid #ff9701; 
    border-radius: 36rpx;
  }
  button.issue::after{
    border:none;
  }
  .fileIfPIC{
    padding-left: 9rpx;
