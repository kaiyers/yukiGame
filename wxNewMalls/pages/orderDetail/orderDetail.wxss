page{
  background:#eee;
}
.header{
  border-top:2rpx solid #e0e0e0;
  background:#fff;
}
.headerInner{
  padding-left:30rpx;
  padding-right: 30rpx;
}
.headerTitle{
  padding-top:26rpx;
  padding-bottom: 22rpx;
  border-bottom:2rpx solid #e0e0e0;
}
.orderHeaderLs,.orderTime{
  font-size: 26rpx;
  line-height: 44rpx;
  color:#666;
}
.orderHeaderRs{
  font-size: 26rpx;
  line-height: 44rpx;
  color: #ff9700;
}
.containTop {
  position:relative;
  padding-top:32rpx;
  border-bottom:2rpx solid #e0e0e0;
}
.orderMPic {
  position:absolute;
  top:50%;
  left:28rpx;
  margin-top:-52rpx;
  width:86rpx;
  height:104rpx;
}
.containTopR{
  width:512rpx;
}
.orderMF {
  text-align:left;
  font-size:26rpx;
  color:#000;
  margin-bottom:8rpx;
  line-height:38rpx;
}
.goodPriceL {
  color:#9f9f9f;
}
.goodPriceL,.goodPriceR {
  font-size:26rpx;
  line-height:86rpx;
}
.orderHeaderR {
  font-size:26rpx;
  line-height:62rpx;
  color:#ff9700;
}
.containBot {
  font-size:26rpx;
  background:#fff;
}
.coin{
  margin-left:30rpx;
  margin-right:30rpx;
  text-align:right;
  font-size:26rpx;
  color:#666;
  height:94rpx;
  line-height:94rpx;
  overflow:hidden;
}
.coinDetail{
  display:inline-block;
  font-size:24rpx;
  color:#000;
  font-weight: 900;
  line-height:24rpx;
}
.addressIcon,.name,.phone{
  display: inline-block;
}
.name{
  margin-left:36rpx;
  margin-right: 36rpx;
}
.personMsg{
  border-top:16rpx solid #eee;
  padding:24rpx 30rpx 0 30rpx;
  background:#fff;
}
.personMsg image{
  width:24rpx;
  height: 32rpx;
  vertical-align: middle
}
.name,.phone{
  font-size:26rpx;
  color:#000;
  line-height: 44rpx;
}
.trueAddress{
  font-size:24rpx;
  color:#9a9a9a;
  line-height: 38rpx;
  padding-bottom:24rpx;
  padding-left: 62rpx;
}
.leaveMsg image,.leaveMsgs{
  display: inline-block;
}
.leaveMsg image{
  width:32rpx;
  height: 32rpx;
  margin-right:32rpx;
  vertical-align: middle;
}
.leaveMsgs{
  font-size:26rpx;
  color:#999;
  line-height: 96rpx;
}
.listMsg,.leaveMsg{
  line-height: 94rpx;
}
.listMsg{
  border-bottom:2rpx solid #e0e0e0;
}
.listMsgT.listMsg{
  border:none;
}
.listMsgContain{
  border-top: 16rpx solid #eee;
  border-bottom:16rpx solid #eee;
}
.leaveMsg,.listMsgContain{
  padding-left:30rpx;
  padding-right: 30rpx;
  margin:0;
  background:#fff;
}
.listMsg view{
  font-size:26rpx;
  color:#000;
}
.seviceTime{
  font-size:26rpx;
  color:#666;
  line-height: 94rpx;
}
.seviceBtn{
  width:686rpx;
  margin:0 auto;
  border-radius: 36rpx;
  border:2rpx solid #ff9700;
  text-align: center;
}
.seviceBtn image,.seviceBtn view{
  display: inline-block;
}
.seviceBtn image{
  width:42rpx;
  height: 42rpx;
  margin-right: 18rpx;
  vertical-align: middle;
}
.seviceBtn view{
  font-size:30rpx;
  color:#ff9700;
  line-height: 76rpx;
}
.seviceBottom{
  padding:0 30rpx 32rpx 30rpx;
  background:#fff;
}
.attchBloak{
  border-top:2rpx solid #e0e0e0;
}
.checks{
  padding:16rpx 30rpx;
  text-align:right;
}
.msgL,.msgR,.confirmReceipt,.reminding,.paying{
  display:inline-block;
  height:60rpx;
  width:188rpx;
  font-size:24rpx;
  border-radius:26rpx;
  text-align:center;
  line-height:60rpx;
}
.msgR,.confirmReceipt {
  color:#ff9700;
  border:2rpx solid #ff9700;
}
.msgR {
  margin-left:16rpx;
}
.msgL,.reminding{
  color:#666;
  border:2rpx solid #666;
}
.paying{
  color:#fff;
  height:64rpx;
  margin-left:16rpx;
  background:#ffc800;
}
.paying span{
  display:inline-block;
  font-size:28rpx;
  height:60rpx;
  line-height:60rpx;
  vertical-align:top;
  margin-right:10rpx;
}
time-compent{
  display:inline-block;
  font-size:28rpx;
  height:60rpx;
  line-height:60rpx;
  vertical-align:middle;
}
.inner{
  color:#fff;
  font-size:28rpx;
  line-height:54rpx;
}
.leaveMsgIn{
  border-top:2rpx solid #e0e0e0;
}
.listMsgT .fr view{
  display: inline-block;
  height: 8rpx;
  line-height: 26rpx;
  border-bottom:16rpx solid #fff0be;
}