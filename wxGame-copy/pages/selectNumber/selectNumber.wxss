.contain{
  position: absolute;
  width:100%;
  height:1222rpx;
  background:url('http://yukicomic-pic.oss-cn-hangzhou.aliyuncs.com/xcx/selectBg.jpg');
  background-repeat: round;
  overflow: hidden;
}
.contain{
  overflow: hidden;
}
.circleContain{
  position: relative;
  width:140rpx;
  height: 140rpx;
  border-radius: 50%;
  margin: 45rpx auto 20rpx;
}
.face{
  position: absolute;
  left:0;
  top:0;
  width:140rpx;
  height:140rpx;
  border-radius:50%;  
  background:#fff;
  transform:scale(0.85);
}
.logo{
  position: absolute;
  left:0;
  top:0;
  width:140rpx;
  height:140rpx;
  border-radius:50%; 
  transform:scale(0.77);
}
.smove{
  position: absolute;
  left:-1rpx;
  top:-1rpx;
  width:140rpx;
  height:140rpx;
  border:1rpx solid #67619d;
   border-radius:50%; 
  transform-origin:50% 50%;
  animation:circleRoate 2s infinite;
  animation-timing-function:linear; 
}
.smove span {
  position:absolute;
  left:50%;
  top:-3rpx;
  margin-left:-3rpx;
  width:8rpx;
  height:8rpx;
  background:#dddce9;
  border-radius:50%;
}
@keyframes circleRoate{  
      from{transform: rotate(0deg) }  
      to{transform: rotate(360deg);}  
  }   
