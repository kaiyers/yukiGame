.italicBlock{
  position:absolute;
  top:50%;
  left:50%;
  margin-left:-360rpx;
  margin-top:-370.5rpx;
  width:720rpx;
  height:741rpx;
  overflow:hidden;
  background-color: #3f3691;
  background:linear-gradient(-45deg, rgba(63, 54, 145, .4) 25%, transparent 25%, transparent 50%, rgba(63, 54, 145, .4) 50%, rgba(63, 54, 145, .4) 75%, transparent 75%, transparent);
  background-size: 10px 10px;
  border-radius: 24rpx;
  z-index:999;
}
@keyframes beforeRoate{  
                0% {
                  transform: scale(1);
                  opacity: 1;
                }  
                100% {
                  transform: scale(0);
                  opacity: 0;
                }
            } 
@keyframes balanceRoate{
                from {
                  transform: scale(0);
                  opacity: 0;
                }  
                to {
                  transform: scale(1);
                  opacity: 1;
                }
            } 
@keyframes ellipsis {
    from {
        width: 2px;
    }
    to {
        width: 22px;
    }
}   
 @keyframes titleIn{ 
  0% {
    transform: scale(5);
    opacity: 0;
    left:650%;
    top:-1000rpx;
  }
  50% {
    transform: scale(1);
    opacity:1;
    left:50%;
    top:0;
  }
  75% {
    transform: scale(0.3);
    opacity:0.3;
    left:50%;
    top:0;
  }
  100% {
    transform: scale(1);
    opacity:1;
    left:50%;
    top:0;
  }
} 
@keyframes titleOut{ 
  0% {
    transform: scale(1);
    opacity:1;
    left:50%;
    top:0;
    
  }
  100% {
    transform: scale(0);
    opacity: 0;
    left:650%;
    top:-1000rpx;
  }
} 
@keyframes peopleIn{ 
  0% {
    margin-left:2000rpx
  }
  50% {
    margin-left:0
  }
  75% {
      margin-left:0;
      transform:rotateX(45deg) ;
      opacity: 0.3
  }
  100% {
    margin-left:0;
    transform:rotateX(0deg);
    opacity: 1
  }
} 
@keyframes peopleOut{ 
  0% {
    margin-left:0;
    opacity: 1
  }
  100% {
    margin-left:2000rpx
  }
} 
@keyframes circleRoate{  
    from{transform: rotate(0deg)}  
    to{transform: rotate(360deg);}  
} 
@keyframes matePeopleIn {
    from {
        margin-left:-1000rpx;
        opacity: 0;
    }
    to {
        margin-left:0;
        opacity: 1;
    }
}   
@keyframes matePeopleOut {
    from {
        margin-left:0;
        opacity: 1;
    }
    to {
        margin-left:-1000rpx;
        opacity: 0;
    }
}
@keyframes headerIn {
    from {
        margin-left:-1000rpx;
    }
    to {
        margin-left:0;
    }
}   
@keyframes newGoodsContainIn {
    from {
        margin-left:1000rpx;
    }
    to {
        margin-left:0;
    }
}   